@model banimo.ViewModel.contactSectionVM



    <footer class="bd-footer   bg-black-80">
        <style>
            .descSecHolder {
                display: flex
            }

            .descSec {
                background-color: #302e2e;
                padding: 20px;
                flex: 1;
                margin: 20px
            }

                .descSec i {
                    font-size: 24px;
                    color: #FF4500;
                    margin-bottom: 15px
                }

                .descSec p {
                    margin: 10px 0;
                }

            .footerH {
                color: #FF4500 !important;
            }

            .footerdiv p {
                color: white;
                margin-bottom: 12px
            }

            .footerdiv a {
                color: white;
                margin-bottom: 10px
            }
        </style>
        <div class="footer maxwidth" style="width:100% ; background-color:#4d4d4d;color:white;  padding-top:50px ">

            <div style="width: 90%; margin: auto; padding-bottom: 35px; border-bottom: 1px #ddd solid;text-align:center">
                <h5 style="color:white;font-size:16px; margin:20px 0 30px 0 ; font-weight:500">دیجی جانبی مرکز اصلی پخش لوازم جانبی و الکترونیکی</h5>
                <h6 style="color:white;margin:30px 0;font-size:13px; direction:rtl">مجموعه ما با هدف افزایش سطح اطلاعات عمومی درباره‌ی سیستم های صوتی بوجود آمد و اکنون مفتخریم که در قالب یک تیم حرفه‌ای بهترین محصولات صوتی از برترین برندها را به ایرانیان عزیز ارائه می‌کنیم.</h6>
                <div class="descSecHolder" style="display:flex">
                    <div class="descSec">
                        <i class="fa fa-truck"></i>
                        <p>ارسال سریع کالا به تمام نقاط ایران</p>
                        <p>تهران 1 الی 3 ساعت | شهرستان 2 الی 3 روز</p>
                    </div>
                    <div class="descSec">
                        <i class="fa fa-check-circle"></i>
                        <p>ارسال سریع کالا به تمام نقاط ایران</p>
                        <p>تهران 1 الی 3 ساعت | شهرستان 2 الی 3 روز</p>
                    </div>
                    <div class="descSec">
                        <i class="fa fa-badge"></i>
                        <p>ارسال سریع کالا به تمام نقاط ایران</p>
                        <p>تهران 1 الی 3 ساعت | شهرستان 2 الی 3 روز</p>
                    </div>
                </div>
            </div>



            <div style="width: 90%; margin: auto; padding-bottom: 35px; border-bottom: 1px #ddd solid">
                <div class="col-lg-12" style="text-align:right; direction:rtl; line-height: 2.1;margin-top:50px">
                    @Html.Raw(@Model.aboutus)
                </div>
                <div style="display:flex;width:100%">
                    <div style="width:30%">
                        <div style=" padding:60px 40px">
                            <div class="row" style="margin:0px; display:flex; margin:0">

                                @{
                                    if (Model.trusteeLink != "")
                                    {
                                        string hrefSRT = "https://trustseal.enamad.ir/?" + Model.trusteeLink;
                                        string srcSRT = "https://Trustseal.eNamad.ir/logo.aspx?" + Model.trusteeLink;
                                        <div class="col-lg-4 col-md-4 col-sm4" style="justify-content:center;  margin:0;padding:5px; display:inline-block; padding:7px; margin-right:3px; background-color:#eee;border-radius: 5px; border: 1px solid #eee;">
                                            <div style="padding:5px; display:flex; object-fit:cover" class="VerifyHolder">
                                                <a target="_blank" href=@hrefSRT>
                                                    <img src=@srcSRT alt="" style="width:100%; cursor:pointer" id=@Model.code>
                                                </a>
                                            </div>
                                        </div>



                                    }
                                    if (Model.samanLink != "")
                                    {
                                        <div class="col-lg-4 col-md-4 col-sm-4" style="justify-content:center;  margin:0;padding:5px; display:inline-block; padding:0px; margin-right:3px; background-color:#eee;border-radius: 5px; border: 1px solid #eee;">
                                            <div style="padding:5px; display:flex; object-fit:cover">
                                                <img id='@Model.samanCode' style='width:100%;cursor:pointer' onclick='window.open("@Model.samanLink", "Popup","toolbar=no, scrollbars=no, location=no, statusbar=no, menubar=no, resizable=0, width=450, height=630, top=30")' alt='logo-samandehi' src=@Model.samanLink2 />
                                            </div>
                                        </div>
                                    }

                                    if (Model.etehadie != "")
                                    {
                                        <div class="col-lg-4 col-md-4 col-sm-4" style="justify-content:center; margin:0;padding:5px; display:inline-block; padding:7px; margin-right:3px; background-color:#eee;border-radius: 5px; border: 1px solid #eee;">

                                            <div style="padding:5px; display:flex; object-fit:cover; justify-content:center" class="VerifyHolder">
                                                <img src="data:image/svg+xml;base64, " alt="" onclick="window.open('@Model.etehadie', 'Popup','toolbar=no, location=no, statusbar=no, menubar=no, scrollbars=1, resizable=0, width=580, height=600, top=30')" style="cursor:pointer; width: 96px;height: 144px;">
                                            </div>
                                        </div>
                                    }
                                }


                            </div>
                        </div>
                        <div class="row" style="">

                            <div style="float:left" class="c-footer__address-images">
                                @*<a href=@Model.googlePlayLink target="_blank" class="c-footer__address-appstore">
                                    <img alt="playstore" width="150" src="~/images/googlePlay.png">
                                </a>*@
                                <a href=@Model.cafeBazarLink target="_blank">
                                    <img alt="" width="150" src="~/images/cafeBazar.png">
                                </a>
                                @*<a href=@Model.sibappLink target="_blank">
                                    <img alt="" width="150px" src="~/images/sibApp.png">
                                </a>*@
                                <a href=@Model.directLink target="_blank">
                                    <img alt="" width="150" src="~/images/direct.png">
                                </a>
                            </div>
                        </div>

                    </div>
                    <div class="footerdiv" style="width: 70%; float: right; margin-top: 25px">
                        <p style=" font-weight:bold;  text-align:right; margin-bottom:15px">تماس با ما</p>
                        <p id="footerPhone" style="white-space: pre-line;    text-align: right; margin-bottom: 15px; direction:rtl; line-height:2 ">شماره تماس : <br> @Model.phone</p>
                        <p style="white-space: pre-line;   text-align: right; margin-bottom: 15px; direction:rtl; line-height:2">  آدرس:<br> @Html.Raw(@Model.address) </p>

                    </div>



                </div>



            </div>




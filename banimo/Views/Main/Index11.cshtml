             data-swiper-options="{
                    'spaceBetween': 15,
                    'slidesPerView': 2.6,
                    'breakpoints': {
                        '576': {
                            'slidesPerView': 3
                        },
                        '768': {
                            'slidesPerView': 4
                        },
                        '992': {
                            'slidesPerView': 5
                        },
                        '1200': {
                            'slidesPerView': 7
                        }
                    }
                }">
            <div class="swiper-wrapper row cols-xl-6 cols-lg-5 cols-md-4 cols-sm-3 cols-2">
                @if (Model.cat.Count() > 1)
                {
                    foreach (var item in Model.cat)
                    {
                        <div idtogo="@item.ID" id="@item.ID-@item.catLevel" class="categoryParent  swiper-slide category category-ellipse">
                            <figure class="category-media">
                                <a href="javascript:void(0)">
                                    <img src="@item.img" alt="Categroy" width="190"
                                         height="190" style="background-color: #5C92C0;" />
                                </a>
                            </figure>
                            <div class="category-content">
                                <h4 class="category-name">
                                    <a href="#">@item.title</a>
                                </h4>
                            </div>
                        </div>
                    }
                }
                else
                {
                    foreach (var item in Model.nodeItems)
                    {
                        <div idtogo="@item.ID" idtoshow="@item.url" class="nodeParent  swiper-slide category category-ellipse">
                            <figure class="category-media">
                                <a href="javascript:void(0)">
                                    <img src="@item.img" alt="Categroy" width="190"
                                         height="190" style="background-color: #5C92C0;" />
                                </a>
                            </figure>
                            <div class="category-content">
                                <h4 class="category-name">
                                    <a href="#">@item.title</a>
                                </h4>
                            </div>
                        </div>
                    }
                }



            </div>
            <div class="swiper-pagination"></div>
        </div>
        <!-- End Of Shadow Swiper -->

        <section class="childcategory  category-section top-category bg-grey pt-10 pb-10 appear-animate" style="margin-top:20px ; display :none; padding-top:30px !important">

            <h2 class="title mb-5 appear-animate" style="direction:rtl;padding-right:30px "> <span id="closeChild" style="cursor:pointer;color:darkviolet">X</span> </h2>
            <div class="container pb-2" style="display:flex; flex-wrap:wrap; justify-content:center" id="childcategoryHolder">

                @if (Model.cat.Count() > 1)
                {
                    if (Model.nodeItems != null)
                    {
                        foreach (var item in Model.nodeItems)
                        {

                            string catmode = "1" + @item.ID;
                            string classname = "p" + @item.cat;
                            <div class="category @classname  categoryitemchild">
                                <div class="swiper-slide  category-classic category-absolute overlay-zoom br-xs" style="height:unset">
                                    <a href="/home/index?partnerID=@item.url" class="category-media">
                                        <img src="@item.img" alt="Category"
                                             width="130" height="130">
                                    </a>

                                </div>

                                <p style="font-size:12px">@item.title</p>
                            </div>

                        }
                    }

                }


                <style>
                    .category {
                        width: 15%;
                        display: inline-block;

@model banimo.ViewModel.MainViewModel.mainIndex

@{
    Layout = "~/Views/Shared/_mainLayout11.cshtml";
}
<style>
    .vendor-widget-banner {
        border-radius: 15px
    }

    .category {
        width: 15%;
        display: inline-block;
        padding: 0 10px;
        border-radius: 8px;
        vertical-align: top
    }

        .category p {
            padding: 20px;
            text-align: center;
            font-weight: 600;
            font-size: 16px;
        }

        .category img {
            border-radius: 10px;
            min-height: 110px
        }

    .gotoallbutton {
        padding: 20px 35px;
        border-radius: 8px;
        border: 1px solid #eee;
        color: black;
        background-color: #A3A3A3;
        cursor: pointer;
        display: none;
    }

        .gotoallbutton:hover {
            background-color: #4d4d4d;
            color: white
        }

    .forweb {
        display: block;
    }

    .forapp {
        display: none;
    }

    .searchHolder {
        display: flex;
        justify-content: center;
        width:90%;
        margin:auto;
    }

    .searchsection {
        background-color: #eee;
        color: #ddd;
        border-radius: 35px;
        display: flex;
        padding: 15px;
        align-items: center;
        width: 90%
    }

        .searchsection i {
            color: #4d4d4d;
            margin: 0 15px
        }

        .searchsection input {
            flex: 1;
            border: none;
            background-color: #eee
        }

    @@media (max-width : 600px) {
        .category {
            width: 33%;
        }

            .category p {
                padding: 8px 0;
                text-align: center;
                font-weight: 600;
                font-size: 12px;
                width: 100%;
                overflow: hidden
            }

            .category img {
                border-radius: 10px;
                min-height: 80px
            }

        .swiper-pagination {
            display: none !important;
        }

        .category-name a {
            font-size: 10px
        }

        .forweb {
            display: none;
        }

        .forapp {
            display: block;
        }

        .btn-group {
            flex-wrap: unset !important;
        }

            .btn-group .btn-wrap {
                min-width: unset !important
            }

                .btn-group .btn-wrap a {
                    margin: 0 10px;
                    font-size: 11px
                }
    }

    .btn-parallax-section {
        padding: 10px 0;
        margin-bottom: 0
    }

    .swiper-slide {
        border-radius: 15px !important;
    }

    .category-wrapper {
        margin-bottom: 1.2rem;
    }
</style>
<main class="main">

    @if (!Request.Browser.IsMobileDevice)
    {
        if (Model.cat.Count() > 1)
        {


            <section class="  btn-section btn-parallax-section parallax" data-parallax-options="{'speed': 10, 'parallaxHeight': '200%', 'offset': -102}" data-image-src="/webAsset/wolmart/assets/images/buttons/banner.jpg" style="background-color: #f8f8f8; position: relative; overflow: hidden;">
                <div class="container">

                    <div class="row">

                        <div class="col-lg-12 col">
                            <div class="btn-group" style="justify-content:center">

                                <div class="btn-wrap show-code-action">
                                    <a href="/" class="btn btn-secondary btn-outline btn-rounded" style="background-color:#5b2871; color:white">Marketplace</a>
                                </div>
                                <div class="btn-wrap show-code-action">
                                    <a href="/Main/Index?catMode=59" id="59-1" class=" btn btn-secondary btn-outline btn-rounded">Food </a>
                                </div>
                                <div class="btn-wrap show-code-action">
                                    @if (Session["LogedInUser"] != null)
                                    {
                                        <a href="/Deliver/index" class="btn btn-secondary btn-outline btn-rounded">Delivery Services </a>

                                    }
                                    else
                                    {
                                        <a href="/Home/login" class="btn btn-secondary btn-outline btn-rounded">Delivery Services </a>

                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        }
        else
        {
            <section class="btn-section btn-parallax-section parallax" data-parallax-options="{'speed': 10, 'parallaxHeight': '200%', 'offset': -102}" data-image-src="/webAsset/wolmart/assets/images/buttons/banner.jpg" style="background-color:#f8f8f8; position: relative; overflow: hidden;">
                <div class="container">
                    @*<h2 class="title title-line text-white mb-2">On Dark Background</h2>*@
                    <div class="row">

                        <div class="col-lg-12 col">
                            <div class="btn-group" style="justify-content:center">

                                <div class="btn-wrap show-code-action">
                                    <a href="/" class="btn btn-secondary btn-outline btn-rounded">Marketplace</a>
                                </div>
                                <div class="btn-wrap show-code-action">
                                    <a href="/Main/Index?catMode=59" id="59-1" class=" btn btn-secondary btn-outline btn-rounded" style="background-color:#5b2871; color:white">Food </a>
                                </div>
                                <div class="btn-wrap show-code-action">
                                    <a href="/Deliver/index" class="btn btn-secondary btn-outline btn-rounded">Delivery Services </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        }
    }



    @if (Request.Browser.IsMobileDevice && Model.cat.Count() == 1)
    {
        <div class="searchHolder row forapp" style="display:flex;padding:15px">
            <div class="searchsection">
                <i class="fa fa-search"></i>
                <input id="searchItem" type="text" ; placeholder="search for restaurant,dishes" />
            </div>
        </div>

        if (Model.tags != null)
        {
            <div style="padding:10px 20px; background-color:#fff">
                <div class="swiper-container swiper-theme nav-inner pg-inner swiper-nav-lg animation-slider pg-xxl-hide pg-show nav-xxl-show nav-hide"
                     data-swiper-options="{
                    'slidesPerView': 4,
                 'spaceBetween': 5,
                    'breakpoints': {
                        '1500': {
                            'nav': true,
                            'dots': false
                        }
                    },
                    'autoplay': {
                        'delay': 8000,
                        'disableOnInteraction': false
                    }
                }">
                    <div class="swiper-wrapper row gutter-no cols-1">
                        @foreach (var item in Model.tags)
                        {
                            int i = Model.tags.IndexOf(item) + 1;


                            <div itemtogo="@item.tag" class="hashtagitem swiper-slide banner banner-fixed intro-slide intro-slide1" style="background-size: contain; background-color: #F7F8FA;height:inherit !important; min-height:unset !important  ">
                                <div style="padding:5px; display:flex; justify-content:center" class="">
                                    <span class="tagitem" style="white-space:nowrap;  text-align:center;border-radius:5px; font-size:12px;">@item.title</span>
                                </div>
                            </div>

                        }



                    </div>

                </div>

            </div>

        }

    }



    <section class="intro-wrapper" style="padding:15px">
        @{
            double cou = 1;
            List<banimo.ViewModel.MainViewModel.Sld> slideLIst = new List<banimo.ViewModel.MainViewModel.Sld>();

            if (Request.Browser.IsMobileDevice)
            {
                cou = 1;
                slideLIst = Model.sld.ToList().Where(x => x.type == "app").ToList();
            }
            else
            {
                slideLIst = Model.sld.ToList().Where(x => x.type != "app").ToList();

            }
        }

        <div class="swiper-container swiper-theme nav-inner pg-inner swiper-nav-lg animation-slider pg-xxl-hide pg-show nav-xxl-show nav-hide"
             data-swiper-options="{
                    'slidesPerView': @cou,
                 'spaceBetween': 15,
                    'breakpoints': {
                        '1500': {
                            'nav': true,
                            'dots': false
                        }
                    },
                    'autoplay': {
                        'delay': 8000,
                        'disableOnInteraction': false
                    }
                }">
            <div class="swiper-wrapper row gutter-no cols-1">
                @foreach (var item in slideLIst)
                {
                    int i = Model.sld.IndexOf(item) + 1;
                    string url = string.IsNullOrEmpty(item.catIDOrLink) ? "" : item.catIDOrLink;
                    <div urltogo="@url" class=" slideitem swiper-slide banner banner-fixed intro-slide intro-slide1" style=" background-image: url(@item.image);    background-size: contain; background-color: #F7F8FA; border-radui; ">
                        <div class="container">

                            <div class="banner-content y-50 d-inline-block">
                                <div class="slide-animate" data-animation-options="{
                                        'name': 'flipInY', 'duration': '1s'
                                    }">
                                    <h5 class="banner-subtitle text-uppercase font-weight-bold ls-10">
                                        @item.Ptitle
                                    </h5>
                                    <h3 class="banner-title text-capitalize font-weight-normal ls-25">
                                        <strong>@item.title</strong><br>
                                    </h3>


                                </div>
                            </div>
                        </div>
                    </div>




                }




            </div>
            <div class="swiper-pagination"></div>
            <button class="swiper-button-next"></button>
            <button class="swiper-button-prev"></button>
        </div>
    </section>
    <!-- End of Intro Wrapper  -->

    <div class="container" style="">





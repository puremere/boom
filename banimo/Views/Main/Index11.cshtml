        @if (Model.cat.Count() > 1)
        {
            <div class="row category-banner-2cols cols-md-2 appear-animate">
                @if (Model.banners != null)
                {
                    string url = Model.banners.SingleOrDefault(x => x.title == "4") == null ? "" : Model.banners.SingleOrDefault(x => x.title == "4").catIDOrLink;
                    string image = Model.banners.SingleOrDefault(x => x.title == "4") == null ? "" : Model.banners.SingleOrDefault(x => x.title == "4").image;

                    <div urltogo="@url" class="slideitem col-md-6 mb-4">

@section scripts{
    <script>
        $(".moreProduct").click(function () {
            var title = $(this).attr('tag')
            window.location.href = "/Home/ProductList?tag=" + title;
        })
        $(".moretag").click(function () {
            var title = $(this).attr('tag')

            $.ajax({
                url: '/Main/setValues',
                dataType: 'html',
                data: {
                    tag: title
                },
                success: function (data) {
                    window.location.href = "/Main/searchResult";
                },
                error: function () {
                    alert("error")
                }


            });
        })
        $(".fa-search").click(function () {
            var title = $("#searchItem").val()

            $.ajax({
                url: '/Main/setValues',
                dataType: 'html',
                data: {
                    query: title
                },
                success: function (data) {
                    window.location.href = "/Main/searchResult";
                },
                error: function () {
                    alert("error")
                }


            });

        })
        $(".hashtagitem").click(function () {
            var title = $(this).attr('itemtogo');

            $.ajax({
                url: '/Main/setValues',
                dataType: 'html',
                data: {
                    tag: title
                },
                success: function (data) {
                    window.location.href = "/Main/searchResult";
                },
                error: function () {
                    alert("error")
                }


            });

        })

        $("#closeChild").click(function () {
            $(".childcategory").slideUp()
        })
        $(".categoryParent").click(function () {
            var id = "p" + $(this).attr('idtogo');

            $(".childcategory").slideDown()
            $(".categoryitemchild").css("display", "none");
            $("." + id).css("display", "inline-block");
        })

        $(".nodeParent").click(function () {
            var id = $(this).attr('idtogo');
            var shoid = $(this).attr('idtoshow');
            var calssname = "p" + shoid;
            $.ajax({
                url: '/Main/getCatPartner',
                dataType: 'html',
                data: {
                    id: id

                },
                success: function (data) {

                    $("#childcategoryHolder").html(data);
                    $(".childcategory").slideDown()
                    $(".categoryitemchild").hide();
                    $("." + calssname).show();



                },
                error: function () {
                    alert("error")
                }


            });
        })

        $(".slideitem").click(function () {
            var url = $(this).attr('urltogo');
            if (url != "") {
                window.location.href = url;
            }
        })
       

        $(".nodeImage").each(function () {
           
            $(this).height($(this).width());
        })
      
        $(document).ready(function () {
            $(".category img").each(function () {
            })
            let widthwhole = $(window).width();
            let height = 0;
            if (widthwhole < 600) {
                height = widthwhole / 6;
            }
            else {
                height = widthwhole / 13;
            }
           
            $(".itemToCheck").each(function () {
               
                $(this).find('figure').height(height);
            })
        })
       
       
        $(".mainImage").each(function () {
            var width = $(this).width();
            var hight = parseInt(width) * 0.7
            $(this).find('img').height(hight);
        })
    </script>

                            .category p {
                                padding: 8px 0;
                                text-align: center;
                                font-weight: 600;
                                font-size: 12px;
                                width: 100%;
                            }
                    }
                </style>
            </div>


            <div>
                @if (Model.cat.Count() > 1)
                {
                    <div style="display:flex; justify-content:center">
                        @if (Model.nodeItems != null)
                        {
                            foreach (var item in Model.nodeItems.GroupBy(x => x.cat).ToList())
                            {
                                string classname = "p" + @item.First().cat;
                                <div id="@item.First().cat-1" class="btn-wrap show-code-action final  @classname categoryitemchild">
                                    <a href="#" class="btn btn-primary btn-outline">See All</a>
                                </div>

                            }
                        }

                    </div>

                }
                else
                {
                    <div style="display:flex; justify-content:center; ">
                        @if (Model.nodeItems != null)
                        {
                            foreach (var item in Model.nodeItems.ToList())
                            {
                                string classname = "p" + @item.url;
                                string pID = item.url.ToString();
                                <div id="" class="btn-wrap show-code-action    categoryitemchild  @classname">
                                    <a href="/Home/index?partnerID=@pID" class="btn btn-primary btn-outline">See More</a>
                                </div>

                            }
                        }

                    </div>

                }

            </div>
        </section>
        <div class="swiper-container swiper-theme category-banner-3cols pt-2 pb-2 mb-10 appear-animate"
             data-swiper-options="{
                    'spaceBetween': 20,
                    'slidesPerView': 1.3,
                    'breakpoints': {
                        '576': {
                            'slidesPerView': 2
                        },
                        '992': {
                            'slidesPerView': 3
                        }
                    }
                }">
            <div class="swiper-wrapper row cols-lg-3 cols-sm-2 cols-1">
                @if (Model.banners.Count() > 0)
                {
                    string url = "";
                    string image = "";
                    int count = Model.banners.Where(x => x.title == "1").ToList().Count();
                    if (count > 0)
                    {
                        url = Model.banners.Where(x => x.title == "1").First().catIDOrLink;
                        image = Model.banners.Where(x => x.title == "1").First().image;
                    }
                    <div urltogo="@url" class=" slideitem swiper-slide banner banner-fixed category-banner">
                        <figure class="br-sm">
                            <img src="@image" alt="Category Banner" width="400"
                                 height="200" style="background-color: #4F4B48;" />
                        </figure>

                    </div>
                    <!-- End of Category Banner -->
                }
                @if (Model.banners.Count() > 1)
                {
                    string url = "";
                    string image = "";
                    int count = Model.banners.Where(x => x.title == "2").ToList().Count();
                    if (count > 0)
                    {
                        url = Model.banners.Where(x => x.title == "2").First().catIDOrLink;
                        image = Model.banners.Where(x => x.title == "2").First().image;
                    }

                    <div urltogo="@url" class=" slideitem swiper-slide banner banner-fixed category-banner">
                        <figure class="br-sm">
                            <img src="@image" alt="Category Banner" width="400"
                                 height="200" style="background-color: #A3A3A3;" />
                        </figure>

                    </div>
                    <!-- End of Category Banner -->
                }
                @if (Model.banners != null)
                {
                    string url = "";
                    string image = "";
                    int count = Model.banners.Where(x => x.title == "3").ToList().Count();
                    if (count > 0)
                    {
                        url = Model.banners.Where(x => x.title == "3").First().catIDOrLink;
                        image = Model.banners.Where(x => x.title == "3").First().image;
                    }
                    <div urltogo="@url" class=" slideitem swiper-slide banner banner-fixed category-banner">
                        <figure class="br-sm">
                            <img src="@image" alt="Category Banner" width="400"
                                 height="200" style="background-color: #151A1E;" />
                        </figure>

                    </div>
                    <!-- End of Category Banner -->
                }




            </div>
            <div class="swiper-pagination"></div>
        </div>
        <!-- End of Category Banner 3Cols -->

        @if (Model.rec != null)
        {
            if (Model.rec.Count() > 0)
            {
                <div class="title-link-wrapper mb-4 appear-animate">
                    <h2 class="title title-link title-vendor pt-2 pb-2"> @Resources.res.RecommendedItems</h2>


                </div>
                <div class="swiper">
                    <div class="swiper-container shadow-swiper swiper-theme vendor-wrapper appear-animate mb-10 pb-1"
                         data-swiper-options="{
                        'spaceBetween': 20,
                        'slidesPerView': 1.3,
                        'breakpoints': {
                            '576': {
                                'slidesPerView': 2
                            },
                            '768': {
                                'slidesPerView': 3
                            },
                            '992': {
                                'slidesPerView': 4
                            }
                        }
                    }">
                        <div class="swiper-wrapper row cols-xl-4 cols-md-3 cols-sm-2 cols-1">
                            @foreach (var spec in Model.rec)
                            {
                                <div class="swiper-slide vendor-widget">
                                    <div class="vendor-widget-banner">
                                        <figure class="vendor-banner">
                                            <a href="/home/index?partnerID=@spec.url">
                                                <img src="@spec.img" alt="Vendor Banner"
                                                     width="1200" height="390" style="background-color: #ECE7DF;" />
                                            </a>
                                        </figure>
                                        <div class="vendor-details">
                                            <figure class="vendor-logo">
                                                <a href="/home/index?partnerID=@spec.url">
                                                    <img src="@spec.logo"
                                                         alt="Vendor Logo" width="90" height="90" />
                                                </a>
                                            </figure>
                                            <div class="vendor-personal">
                                                <h4 class="vendor-name">
                                                    <a href="/home/index?partnerID=@spec.url">@spec.title</a>

    <div class="slider-area">
        <div class="slider-active owl-carousel">
            @{
                string title0 = "";
                string title1 = "";
                string title2 = "";
                string title3 = "";
                string title4 = "";
                string desc0 = "";
                string desc1 = "";
                string desc2 = "";
                string desc3 = "";
                string desc4 = "";

                foreach (var item in Model.slides)
                {
                    int index = Model.slides.IndexOf(item);
                    string title = "";
                    string desc = "";

                    switch(index)
                    {
                        case 0:
                            title = title0;
                            desc = desc0;
                            break;
                        case 1:
                            title = title1;
                            desc = desc1;
                            break;
                        case 2:
                            title = title2;
                            desc = desc2;
                            break;
                        case 3:
                            title = title3;
                            desc = desc3;
                            break;
                        case 4:
                            title = title4;
                            desc = desc4;
                            break;

                    }
                    <div class="single-slider slider-1 gray-bg">
                        <div class="container">
                            <div class="row" style="direction:rtl">
                                <div class="col-md-6" style="text-align:right">
                                    <div class="slider-content slider-animated-1">
                                        <h2 class="animated">@title</h2>
                                        <p class="">@desc</p>
                                      
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="slider-single-img slider-animated-1">
                                        <img class="animated" src="~/images/panelimages/@item.image" alt="slider images">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
                }


        </div>

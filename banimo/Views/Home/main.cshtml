    <script>
        function validateEmail(email) {
            const re = /^(([^<>()[\]\\.,;:\s@@"]+(\.[^<>()[\]\\.,;:\s@@"]+)*)|(".+"))@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
            return re.test(String(email).toLowerCase());
        };
        $("#registerEmail").click(function () {
           
            $("#registerEmail").prop("disabled", true);
           
            var email = $("#registeremail").val();
            if (validateEmail(email)) {
                $.ajax({
                    url: "/Home/registerEmail",
                    val: {

                        form_email: email,

                    },
                    success: function (result) {
                        toastr.options = {
                            "debug": false,
                            "positionClass": "toast-top-center",
                            "onclick": null,
                            "fadeIn": 300,
                            "fadeOut": 1000,
                            "timeOut": 5000,
                            "extendedTimeOut": 10000
                        }
                        toastr.success('Your email was registerd! you1`ll be informed soon');
                        $("#registeremail").val("");
                        $("#registerEmail").prop("disabled", false);

                    }
                });
            }
            else {
                toastr.options = {
                    "debug": false,
                    "positionClass": "toast-top-center",
                    "onclick": null,
                    "fadeIn": 300,
                    "fadeOut": 1000,
                    "timeOut": 5000,
                    "extendedTimeOut": 10000
                }
                toastr.error('Pleas enter valid email address');
            }
            
            
        })
        $("#submitForm").click(function () {
            $("#submitForm").prop("disabled",true)
            var name = $("#form-name").val();
            var email = $("#form-email").val();
            var text = $("#form-message").val();
            $.ajax({
                url: "/Home/setNewUser",
                val: {
                    form_name: name,
                    form_email: email,
                    form_message : text
                },
                success: function (result) {
                    toastr.options = {
                        "debug": false,
                        "positionClass": "toast-top-center",
                        "onclick": null,
                        "fadeIn": 300,
                        "fadeOut": 1000,
                        "timeOut": 5000,
                        "extendedTimeOut": 10000
                    }
                    toastr.success('Thanks for sharing with us! We`ll be in touch soon');
                    $("#form-name").val("");
                    $("#form-email").val("");
                    $("#form-message").val("");
                    $("#submitForm").prop("disabled", false)

                }
            });


        })
    </script>
